{
    "vscode_database_client.connections": [
      {
        "name": "mysql",
        "type": "${env:DB_DRIVER}",
        "host": "${env:DB_HOST}",
        "port": "${env:DB_PORT}",
        "user": "${env:DB_USER}",
        "password": "${env:DB_PASSWORD}",
        "database": "${env:DB_DATABASE}"
      }
    ]
  }